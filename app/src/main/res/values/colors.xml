<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="white">#FFFFFFFF</color>

    <!-- Background Colors -->
    <color name="background_color">#f5f5f1</color> <!-- Soft off-white for background -->
    <color name="background_color_start">#FAFAF7</color> <!-- Gradient Start -->
    <color name="background_color_end">#EDEDEA</color> <!-- Gradient End -->

    <!-- Card Background -->
    <color name="card_background">#E5E5E5</color> <!-- Light grey for card background -->

    <!-- Text Colors -->
    <color name="text_color_primary">#424b39</color> <!-- Darker tone matching the bottom_nav -->
    <color name="text_color_secondary">#7D8573</color> <!-- Complementary muted greenish tone -->

    <!-- Bottom Navigation -->
    <color name="bottom_nav_background">#969d71</color> <!-- Muted green you provided -->
    <color name="bottom_nav_icon">#FFFFFF</color> <!-- White for icons -->
    <color name="bottom_nav_text">#FFFFFF</color> <!-- White for text -->

    <!-- Button colors -->
    <color name="button_color">#e76f51</color> <!-- Warm Coral for buttons -->
    <color name="button_start">#e76f51</color> <!-- Gradient start for buttons -->
    <color name="button_end">#f4a261</color> <!-- Gradient end for buttons -->



    <!-- Progress Bar Colors -->
    <color name="progress_start">#F57C00</color> <!-- Bright Orange -->
    <color name="progress_center">#FFB74D</color> <!-- Lighter Orange -->
    <color name="progress_end">#42A5F5</color> <!-- Soft Blue for contrast -->

    <!-- Hint Text -->
    <color name="hint_color">#B2B2A3</color> <!-- Light muted greyish green for hints -->

    <!-- Toolbar/Action Bar Colors -->
    <color name="colorPrimary">#698B69</color> <!-- Muted greenish grey for toolbar -->
    <color name="colorPrimaryDark">#202020</color> <!-- Dark grey for Toolbar -->
    <color name="colorAccent">#42A5F5</color> <!-- Soft blue accent for contrast -->

    <!-- Additional Colors -->
    <color name="your_desired_color">#673AB7</color> <!-- Purple accent you may want to use elsewhere -->
</resources>





